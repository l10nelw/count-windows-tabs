{
  "manifest_version": 2,
  "name": "Windows and Tabs Counter",
  "description": "Indicate number of open windows and number of tabs in active window",
  "version": "1.0.0",
  "icons": {
    "64": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icon.png"
    },
    "default_title": "Windows and Tabs Counter"
  }
}
